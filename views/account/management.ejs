<%- messages() %>
<% if (errors) { %>
    <ul class="notice">
        <% errors.array().forEach(error => { %>
            <li><%= error.msg %></li>
        <% }) %>
    </ul>
<% } %>

<h2>Welcome <%= userData.account_firstname %></h2>
<% if (userData.account_type === 'Employee' || userData.account_type === 'Admin') { %>
  <h3>Inventory Management</h3>
  <p><a href="/inventory/">Manage Inventory</a></p>
<% } %>
<a href="/account/update">Update Account Information</a>
